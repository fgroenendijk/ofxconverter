<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<shortcuts>
    <skipIfNotSupported/>
    <programGroup defaultName="Ofx converter" location="applications"/>

    <shortcut name="Ofx converter"
        target="/usr/bin/java"
        commandLine="-jar $INSTALL_PATH/OFXConverter.jar"
        description="Launch OFX Converter"
        iconFile="$INSTALL_PATH/icon.png"
        iconIndex="0"
        workingDirectory="$INSTALL_PATH"
        initialState="normal"
        KdeSubstUID="false"
        programGroup="yes"
        desktop="yes"
        applications="no"
        startMenu="yes"
        startup="no"
        type="Application"
        terminal="true"
        encoding="UTF-8">
            <createForPack name="Core"/>
        </shortcut>


</shortcuts>
